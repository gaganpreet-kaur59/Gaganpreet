#include <iostream>
#include<algorithm>
using namespace std;

 int main() {
int arr[]={1,2, 2, 2, 3, 4, 5, 2, 6, 4,7};
int n-sizeof(arr)/sizeof(arr[0]);
sort(arr,arr+n);
int newArr[n];

int i,j=0;
for(i=0;i<n;i++)
{
if(arr[i]==arr[i+1])
{

}
else
newArr[j++]=arr[i];
}
 cout<<"your array with all unique elemnts:"<<endl;
for(i=0;i<j;i++)
{
cout<<newArr[i]<<" ";
}
return 0;
}
