#include <iostream>
using namespace std;

int main() {
int arr[][2]={{1,2}, {3,4), (5,6}};
int trans[2][3];

int i,j;
for(i=0;i<3;i++)
{
for(j=0;j<2;j++)
trans[j][i]=arr[i][j];
}

cout<<"the transposed matrix is: "<<endl;
for(i=0;i<2;i++)
{
for(j=0;j<3;j++)
cout<<trans[i][j]<<" ";
cout<<endl;
}
return 0;
}
