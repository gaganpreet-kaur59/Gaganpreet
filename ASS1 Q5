#include <iostream>
using namespace std;

 int main() {
int arr[] [2]={(1,2), (3,4), (5,6)};
int i,j,sum;

for(i=0;i<3;i++)
{
sum=0;
for(j=0;j<2;j++)
{
sum+=arr[i][j];
}
cout<<"sum of row number "<<i+1<<"="<<sum<<endl;
}
for(i=0;i<2;i++)
{
sum=0;
for(j=0;j<3;j++)
{
sum+=arr[j][1];
} 
cout<<"sum of column number "<<i+1<<"="<<sum<<endl;
}
return 0;
}
