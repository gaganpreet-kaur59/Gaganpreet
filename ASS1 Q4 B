#include <iostream>
using namespace std;

int main() {
int arr1[][3]={{1,2,3}, {4,5,6}};
int arr2[][2]={{1,2}, {3,4}, {5,6}};
int res [2] [2];

int i,j,k;
for(i=0;i<2;i++)
{
for(j=0;j<2;j++)
{
res[i][j]=0;
for(k=0;k<3;k++)
{
res[i][j]+=arr1[i][k]*arr2[k][j];
}
}
}

for(i=0;i<2;i++)
{
for(j=0;j<2;j++)
{
cout<<res[i][j]<<" ";
}
 cout<<endl;
}
return 0;
}
